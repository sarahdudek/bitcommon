{
    "name": "madmis/exchange-api",
    "description": "Common functions for cryptocurrency exchange api",
    "keywords": [
        "exchange",
        "cryptocurrency",
        "rest",
        "api"
    ],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^7.1",
        "guzzlehttp/psr7": "^1.4",
        "guzzlehttp/guzzle": "^6.3",
        "symfony/serializer": "^3.3",
        "symfony/property-access": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "madmis\\": "src",
            "Test\\": "tests"
        }
    },
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^6.2"
    }
}
